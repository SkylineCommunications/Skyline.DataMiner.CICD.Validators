{
  "build": {
    "content": [
      {
        "files": [
          "checks/**.md",
          "checks/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Skyline.DataMiner.CICD.Tools.Validator Documentation",
      "_appFooter": "Â© Skyline Communications 2025",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	    "modern",
      "templates/skyline"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
